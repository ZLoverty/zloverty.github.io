function swim_pressure
h = linspace(1, 128);
SR = 5:5:30;
hold on;
for j = 1: numel(SR)
    for i = 1: numel(h)
        x(i) = visc_Brady(h(i), SR(j));
    end
    plot(h, x);
end
hold off;
end

function visc = visc_Brady(h, SR)
vfrac = 0.01;
A = 1+2.5*vfrac;
B = -3*vfrac/16;
U0 = 30;
a = 3;
Pe = SR*h/U0;
Pe_R = a/h;
visc = A + B*(1/Pe_R)^2*(1-(Pe/4)^2)/(1+(Pe/4)^2)^2;
end
